    "@rdfjs/fetch-lite": "^2.0.1",
    "@rdfjs/formats-common": "^2.0.1",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/parser-jsonld": "^1.2.0",
    "@tpluscode/rdfine": "^0.3.2",
    "@zazuko/rdfine-data-cube": "^0.0.0",
    "alcaeus": "^1.0.0-alpha.9",
    "aws-sdk": "^2.559.0",
    "barnard59": "^0.0.7",
    "barnard59-formats": "0.0.6",
    "clownface": "^0.12.1",
    "commander": "^4.1.0",
    "debug": "^4.1.1",
    "rdf-ext": "^1.3.0",
    "readable-stream": "^3.5.0",
    "string-to-stream": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/rdf-ext": "^1.3.5",
    "@types/rdf-js": "^2.0.11",
