    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__sink-map": "^1.0.0",
    "@types/readable-stream": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "standard": "^12.0.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
