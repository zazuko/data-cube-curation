{
  "name": "@zazuko/data-cube-curation",
  "version": "0.0.0",
  "description": "Runs a pipeline to create an RDF data cube",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir -p input; mkdir -p output",
    "precommit": "lint-staged",
    "start": "ts-node index.ts transform --from filesystem --to filesystem --project https://datacube.zazukoians.org/project/2619f35e-747e-493c-9398-ed88b62a3133 --debug -v sourceDir=input -v targetFile=output/everything.nt",
    "build": "tsc"
  },
  "dependencies": {
