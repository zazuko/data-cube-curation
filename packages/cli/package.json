{
  "name": "data-cube-curation-cli",
  "version": "0.0.0",
  "description": "Runs a pipeline to create an RDF data cube",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir -p input; mkdir -p output",
    "precommit": "lint-staged",
    "start": "dotenv -- npm run start:transform",
    "start:transform": "ts-node index.ts transform --from s3 --to stdout --project $TRANSFORM_PROJECT --debug -v s3endpoint=$S3_ENDPOINT -v s3bucket=$S3_BUCKET -v graph-store-endpoint=$GRAPH_STORE_ENDPOINT -v graph-store-user=$GRAPH_STORE_USER -v graph-store-password=$GRAPH_STORE_PASSWORD -v graph=$TRANSFORM_GRAPH",
    "build": "tsc"
  },
  "dependencies": {
    "@rdfine/csvw": "^0.0.0",
    "@rdfjs/fetch-lite": "^2.0.1",
    "@rdfjs/formats-common": "^2.1.0",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/parser-jsonld": "^1.2.0",
    "@tpluscode/rdfine": "^0.3.5",
    "@zazuko/rdfine-data-cube": "^0.0.0",
    "alcaeus": "^1.0.0-alpha.9",
    "aws-sdk": "^2.559.0",
    "barnard59": "^0.0.9",
    "barnard59-base": "^0.0.3",
    "barnard59-formats": "0.0.6",
    "barnard59-graph-store": "^0.0.4",
    "clownface": "^0.12.1",
    "commander": "^4.1.1",
    "debug": "^4.1.1",
    "duplex-to": "1.0.0",
    "isstream": "^0.1.2",
    "null-writable": "^1.0.5",
    "rdf-ext": "^1.3.0",
    "readable-stream": "^3.6.0",
    "string-to-stream": "^3.0.1",
    "through2-reduce": "1.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/isstream": "^0.1.0",
    "@types/rdf-ext": "^1.3.5",
    "@types/rdf-js": "^2.0.11",
    "@types/rdfjs__formats-common": "^2.0.0",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__sink-map": "^1.0.0",
    "@types/readable-stream": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "dotenv-cli": "3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "standard": "^12.0.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zazuko/data-cube-curation.git"
  },
  "keywords": [
    "rdf",
    "data",
    "cube"
  ],
  "author": "Tomasz Pluskiewicz <tomasz.pluskiewicz@zazuko.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/data-cube-curation/issues"
  },
  "homepage": "https://github.com/zazuko/data-cube-curation/tree/master/cli#readme"
}
