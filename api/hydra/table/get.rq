BASE <${this.env.BASE_URI}>
PREFIX api: <https://rdf-cube-curation.described.at/api/>
PREFIX dataCube: <https://rdf-cube-curation.described.at/>

CONSTRUCT {
    ?table ?p ?o .
    ?table api:attributes ?attributes .
    ?table api:csvwMetadata ?csvw .
}
WHERE {
    BIND (<${this.locals.resourceId}> as ?table)
    BIND (<${this.locals.resourceId}/attributes> as ?attributes)
    BIND (<${this.locals.resourceId}/csvw> as ?csvw)

    ?table
        a dataCube:Table ;
        ?p ?o .
}
