BASE <https://rdf-cube-curation.described.at/>

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX hydra: <http://www.w3.org/ns/hydra/core#>

CONSTRUCT {
    ?collection
        hydra:manages [
            hydra:property rdf:type ;
            hydra:object <DataCubeSource>
        ] , [
            hydra:subject <${this.locals.projectId}> ;
            hydra:property <DataCubeProject/source>
        ].
}
WHERE {
    BIND (<${this.locals.projectId}/sources> as ?collection)
}
